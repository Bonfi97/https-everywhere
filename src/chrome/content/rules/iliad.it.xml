<!--
All other subdomains are either internal/test pages (they give 
403 error or SSL_ERROR_BAD_CERT_DOMAIN error because of their test 
certificate), redirects to corporate.iliad.it (which automatically 
switch you to https), corporate.iliad.it (which automatically switch you
to https) or registrazione.iliad.it (which automatically switch you to
https).
-->
<ruleset name="iliad.it">
        <target host="iliad.it" />
        <target host="www.iliad.it" />

        <rule from="^http:" to="https:" />
</ruleset>
